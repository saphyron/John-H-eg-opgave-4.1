@{
    ViewData["Title"] = "Find Location";
}

<div class="text-center">
    <div>
        <form id="road-search">
            <div class="form-group">
                <label for="RoadSearch">
                    Input characters for Road Search
                </label>
                <input name="RoadSearch" class="roadSearch" type="text" />
                <input type="submit" value="All roads" />
            </div>
        </form>
        <form id="road-code">
            <div class="form-group">
                <label for="RoadCode">
                    Input characters for Road Code
                </label>
                <input name="RoadCode" class="roadCode" type="text" />
            </div>
            <div class="form-group">
                <label for="KommuneCode">
                    Input characters for Road Search
                </label>
                <input name="KommuneCode" class="kommuneCode" type="text" />
                <input type="submit" value="Road Search" />
            </div>
        </form>
    </div>
    <div>
        <div class="list-content">
            <div id="list-view">
                
            </div>

        </div>
    </div>
    
</div>

@section scripts{
    @{
        <script src="../js/roadfunctions.js" type="text/javascript"></script>
    }
}